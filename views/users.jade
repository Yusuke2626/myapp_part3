extends layout

block content
  p.tittle ユーザー情報
  
  
  form(action='/users' method='post' enctype='multipart/form-data')
    p ユーザー名を入力
      input(type='text' name='name')
    p emailを入力
      input(type='text' name='email')
    p 画像を登録
      input(type='file' name='img')  
    input(type='submit')
